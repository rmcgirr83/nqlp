{
    "name": "tojag/nqlp",
    "type": "phpbb-extension",
    "description": "No quote last post",
    "version": "1.0.2",
    "time": "2024-07-17",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "RMcGirr83",
            "role": "Developer"
        },
        {
            "name": "tojag",
            "role": "Previous author"
        }
    ],
	"require": {
        "php": ">=7.4",
        "phpbb/phpbb": ">=3.3",
        "composer/installers": "~1.0"
	},
    "extra": {
        "display-name": "No quote last post",
        "soft-require": {
            "phpbb/phpbb": ">=3.3"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/no_quote_last_post",
            "filename": "version_check",
            "ssl": true
        }
    }
}